{#
/**
 * @file
 * Theme override for a 'form' element.
 *
 * Available variables
 * - attributes: A list of HTML attributes for the wrapper element.
 * - children: The child elements of the form.
 *
 * @see template_preprocess_form()
 */
#}

{{ attach_library('nc_theme/layout_builder') }}

<div class="form-container container mt-7 mb-7">
  <div class="row">
    <form{{ attributes.addClass('layout-builder-form') }}>
      {{ children }}
    </form>
  </div>
</div>
